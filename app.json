{
  "expo": {
    "name": "NinzaGames",
    "slug": "NinzaGames",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-images/app.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo-images/app.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-images/app.png"
      },
      "package": "com.skyairey.NinzaGames"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-images/app.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "193602d3-07e8-429f-8d37-7fb739ecc447"
      }
    }
  }
}
